<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pathActivity"
    android:orientation="vertical">
    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="0dp"
            android:id="@+id/path_num_id"/>
        <ImageView
            android:layout_width="276dp"
            android:layout_height="74dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/finallogo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </android.support.constraint.ConstraintLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background"
        android:orientation="vertical"
        android:layout_weight="1"
        android:id="@+id/path_linear">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@drawable/layoutshape"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:paddingTop="8dp"
            android:paddingLeft="15dp"
            android:paddingRight="15dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/roter_text"
                android:layout_weight="1"
                android:textSize="16dp"
                android:id="@+id/starttext"
                android:layout_marginTop="-5dp"/>


            <EditText
                android:layout_width="155dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/start"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="-5dp"
                android:scrollHorizontally="false"
                android:singleLine="true"
            />
            <!--<TextView-->
            <!--android:layout_width="182dp"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_weight="1"-->
            <!--android:id="@+id/start"/>-->
        <!--<ImageView-->
            <!--android:layout_width="40dp"-->
            <!--android:layout_height="40dp"-->
            <!--android:src="@drawable/select"-->
            <!--android:layout_weight="1"-->
            <!--android:id="@+id/gps"/>-->
            <Button
                android:layout_width="50dp"
                android:layout_height="35dp"
                android:background="@drawable/buttonshape3"
                android:text="@string/select"
                android:textColor="#ffffff"
                android:textSize="16sp"
                android:layout_marginLeft="6dp"
                android:id="@+id/select1"
                android:layout_marginTop="-5dp"
                />
</LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@drawable/layoutshape"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:paddingTop="8dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/roter_last"
            android:layout_weight="1"
            android:textSize="16dp"
            android:id="@+id/endtext"
            android:layout_marginTop="-5dp"/>

        <EditText
            android:layout_width="155dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/end"
            android:imeOptions="actionSearch"
            android:inputType="text"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="-5dp"
            android:scrollHorizontally="true"/>
        <!--<TextView-->
            <!--android:layout_width="182dp"-->
